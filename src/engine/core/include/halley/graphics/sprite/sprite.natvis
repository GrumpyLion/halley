	  <DisplayString Condition="hotReloadRef != nullptr &amp;&amp; dynamic_cast&lt;const Halley::SpriteResource*&gt;(hotReloadRef) != nullptr">{{pos:{vertexAttrib.pos}, size:{vertexAttrib.size}, ref:{((Halley::SpriteResource*)hotReloadRef)->assetId}, material:{*material}}}</DisplayString>
	  <DisplayString Condition="material._Ptr">{{pos:{vertexAttrib.pos}, size:{vertexAttrib.size}, material:{*material}}}</DisplayString>
	  <DisplayString>{{pos:{vertexAttrib.pos}, size:{vertexAttrib.size}, material:{{}}}}</DisplayString>
  </Type>
</AutoVisualizer>